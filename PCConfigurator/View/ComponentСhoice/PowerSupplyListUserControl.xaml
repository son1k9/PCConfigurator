<UserControl x:Class="PCConfigurator.View.ComponentСhoice.PowerSupplyListUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PCConfigurator.View.ComponentСhoice"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DataGrid Style="{StaticResource componentsList}" Name="dataGrid">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Model}" Header="Модель"/>
                <DataGridTextColumn Binding="{Binding Wattage}" Header="Мощность"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
